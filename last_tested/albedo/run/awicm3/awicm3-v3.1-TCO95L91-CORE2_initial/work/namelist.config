&modelname
    runid = 'fesom'
/

&timestep
    step_per_day = 36
    run_length = 1
    run_length_unit = 'd'
/

&clockinit
    timenew = 0.0
    daynew = 1
    yearnew = 2000
/

&paths
    meshpath = 'TO-BE-DEFINED-BY-THE-USER/fesom2//core2/'
    climatedatapath = 'TO-BE-DEFINED-BY-THE-USER/fesom2//hydrography/'
    resultpath = '<TEST_DIR>/run/awicm3//awicm3-v3.1-TCO95L91-CORE2_initial/run_20000101-20000101/work/'
/

&restart_log
    restart_length = 1
    restart_length_unit = 'd'
    logfile_outfreq = 960
/

&ale_def
    which_ale = 'zstar'
    use_partial_cell = .true.
/

&geometry
    cartesian = .false.
    fplane = .false.
    cyclic_length = 360
    rotated_grid = .true.
    force_rotation = .true.
    alphaeuler = 50.0
    betaeuler = 15.0
    gammaeuler = -90.0
/

&calendar
    include_fleapyear = .true.
/

&run_config
    use_ice = .true.
    use_cavity = .false.
    use_cavity_partial_cell = .false.
    use_floatice = .false.
    use_sw_pene = .true.
/

&machine
    n_levels = 2
    n_part = 12, 36
/

&inout
    restartflag = 'last'
    output_length = 1
    output_length_unit = 'd'
/
