&modelname
    runid = 'fesom'
/

&timestep
    step_per_day = 48
    run_length = 1
    run_length_unit = 'm'
/

&clockinit
    timenew = 0.0
    daynew = 1
    yearnew = 2001
/

&paths
    meshpath = '/albedo/pool/CORE2/'
    climatedatapath = '/albedo/pool/hydrography/'
    resultpath = '<TEST_DIR>/run/fesom//fesom2.0-initial-monthly/run_20010101-20010131/work/'
    forcingdatapath = '/albedo/pool/forcing/'
/

&initialization
    oceclimadataname = 'Winter_PHC3_ts.out'
    use_prepared_init_ice = .false.
/

&inout
    restartflag = 'last'
    output_length = 1
    output_length_unit = 'm'
    output_offset = 64
    restart_length = 1
    restart_length_unit = 'm'
    restart_offset = 64
    logfile_outfreq = 100
    use_means = .false.
/

&mesh_def
    grid_type = 1
    use_ale = .true.
    which_ale = 'linfs'
    use_partial_cell = .false.
    min_hnode = 0.25
    lzstar_lev = 3
/

&geometry
    cartesian = .false.
    fplane = .false.
    betaplane = .false.
    f_fplane = -0.00014
    beta_betaplane = 2e-11
    cyclic_length = 360.0
    rotated_grid = .true.
    alphaeuler = 50.0
    betaeuler = 15.0
    gammaeuler = -90.0
    force_rotation = .false.
/

&calendar
    include_fleapyear = .false.
/

&run_config
    use_ice = .true.
    use_floatice = .false.
    use_sw_pene = .true.
    toy_ocean = .false.
/

&machine
    n_levels = 3
    n_part = 2, 4, 36
/
